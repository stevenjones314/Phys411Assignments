      "hourdata=np.genfromtxt('http://web.uvic.ca/~jklymak/Phy411/Data/AllHourly.txt')[[6,28],2:]\n",
      "dc = hourdata[0,:]\n",
      "jb = hourdata[1,:]\n",
      "\n",
      "dc_raw = dc\n",
      "jb_raw = jb\n",
      "\n",
      "print np.shape(dc_raw)"
     ],
     "language": "python",
     "metadata": {},
     "outputs": [
      {
       "output_type": "stream",
